generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Lead {
  id           Int      @id @default(autoincrement())
  name         String
  phone        String
  contactType  ContactType
  status       LeadStatus @default(PENDING)
  createdAt    DateTime  @default(now())
}

enum ContactType {
  PHONE
  WHATSAPP
  TELEGRAM
}

enum LeadStatus {
  PENDING
  PROCESSED
} 